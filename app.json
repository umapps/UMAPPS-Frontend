{
  "expo": {
    "name": "UM Connect",
    "description": "App to register and book on the UM platform",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.umapps.booking",
      "buildNumber": "1.0.1"
    },
    "notification": {
      "icon": "./assets/icons8-notification-96.png",
      "color": "#FE4040",
      "androidMode": "default"
    },
    "android": {
      "package": "com.umapps.booking",
      "googleServicesFile": "./google-services.json",
      "versionCode": 11,
      "permissions": []
    },
    "slug": "UM-Application",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/fonts/*",
      "assets/icon.png",
      "assets/splash.png"
    ],
    "sdkVersion": "37.0.0"
  }
}